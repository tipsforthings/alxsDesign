<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <li class="">
  <%= link_to "Forgotten Password?", new_password_path(resource_name), :class => "button tiny info" %><br />
  </li>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <li class="">
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), :class => "button tiny secondary" %><br />
  </li>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <li class="">
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), :class => "button tiny secondary tiny radius" %><br />
  </li>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
   <li class="">
   <%= link_to "Sign In with #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider), :class => "button tiny secondary tiny radius" %><br />
    </li>
  <% end -%>
<% end -%>

