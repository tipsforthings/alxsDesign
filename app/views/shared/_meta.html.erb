<title><%= full_title(yield(:title)) %></title>

<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="description" content="<%= full_desc(yield(:desc)) %>">
<meta name="keywords" content="<%= keys(yield(:keys)) %>">
<meta name="author" content="<%= author(yield(:author)) %>">
<meta property="og:url" content="http://alxs.co.uk/<%= content_for?(:path) ? yield(:path) : "" %>" />
<meta property="og:image" content="<%= og_image(yield(:ogimage)) %>" >
<meta property="og:description" content="<%= full_desc(yield(:desc)) %>">
<meta property="og:title" content="<%= full_title(yield(:title)) %>">
<meta property="og:type" content="website" />

